@{
 ViewData["Title"] = "Login";
}
<h2>Login (teste)</h2>
<form asp-action="Login" method="post">
 <div>
 <label>Email</label>
 <input type="email" name="email" />
 </div>
 <div>
 <label>Senha</label>
 <input type="password" name="senha" />
 </div>
 <div>
 <button type="submit">Entrar</button>
 </div>
</form>